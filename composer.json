{
    "name": "teleclient/telegram-api",
    "description": "A complete Telegram bot API implementation written in PHP, with support for inline bots!",
    "keywords": [
        "Telegram",
        "API",
        "Telegram Bot"
    ],
    "authors": [
        {
            "name": "Camilo Sperberg",
            "email": "me+tgapi@unreal4u.com",
            "homepage": "https://github.com/teleclient/telegram-api/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "unreal4u/dummy-logger": "^1.0",
        "react/promise": "^2.7",
        "clue/block-react": "^1.3",
        "yoshi2889/multipart-builder": "^0.1.1",
        "amphp/artax": "^3",
        "react/http": "^1.1"
    },
    "require-dev": {
        "monolog/monolog": "^1.17",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^6.2",
        "phpunit/php-code-coverage": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "unreal4u\\TelegramAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "teleclient\\TelegramAPI\\tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/teleclient/telegram-api/issues",
        "source": "https://github.com/teleclient/telegram-api"
    },
    "license": "MIT"
}